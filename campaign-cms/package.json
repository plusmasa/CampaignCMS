{
  "name": "campaign-cms",
  "version": "1.0.0",
  "description": "Campaign Management CMS",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "frontend": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend\"",
    "reset-db": "node backend/reset-db.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:frontend": "cd frontend && npm test",
    "lint": "eslint backend/",
    "lint:fix": "eslint backend/ --fix",
    "format": "prettier --write backend/**/*.js",
    "type-check": "cd frontend && npm run type-check",
    "build": "npm run frontend:build"
  },
  "keywords": [
    "campaign",
    "cms",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
